CC = gcc
MIPS-CC = /opt/buildroot-gcc342/bin/mipsel-linux-gcc 

JSON_C_DIR=user

FLAGS += -I$(JSON_C_DIR)/include/json -I$(JSON_C_DIR)/include/ -I$(JSON_C_DIR)/include/i386-linux-gnu

LDFLAGS+= -Wall -pthread -L$(JSON_C_DIR)/lib -ljson 

SRC = $(wildcard *.c)
OBJ = e2s

$(OBJ): $(SRC)
	$(MIPS-CC)  $(FLAGS) $(LDFLAGS) -o $@ $^

clean:
	rm -rf *~ *.o server e2s SOURCEINSIGHT
	
server:	$(SRC)
	gcc -o json-demo $(FLAGS) test1.c -std=c99 $(LDFLAGS)
#	$(CC)  -o $@  $(SRC) $(FLAGS) $(LDFLAGS)
