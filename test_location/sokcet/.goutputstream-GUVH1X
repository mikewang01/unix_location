CC = gcc
MIPS-CC = /opt/buildroot-gcc342/bin/mipsel-linux-gcc 

JSON_C_DIR=/usr
INCFLAGS += -I/usr/include/json-c -I/usr/include/i386-linux-gnu/ -I/usr/include/
LDFLAGS+= -Wall -pthread - _SVID_SOURCE -std=c99 -L/usr/lib -ljson 
SRC = $(wildcard *.c)
OBJ = e2s

$(OBJ): $(SRC)
	$(MIPS-CC)  $(INCFLAGS) $(LDFLAGS) -o $@ $^

clean:
	rm -rf *~ *.o server e2s SOURCEINSIGHT
	
server:	$(SRC)
	$(CC)  $(LDFLAGS) -o $@  $(SRC)
